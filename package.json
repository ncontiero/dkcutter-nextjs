{
  "name": "dkcutter-nextjs",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.17.1",
  "description": "DKCutter NextJs is a framework for quickly starting Next.Js projects.",
  "author": "Nicolas Contiero <https://github.com/ncontiero>",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "generate": "dkcutter .",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "check": "pnpm lint && pnpm typecheck",
    "test": "vitest",
    "commitlint": "commitlint --edit",
    "pre-commit": "lint-staged -c ./package.json",
    "prepare": "husky"
  },
  "devDependencies": {
    "@clerk/nextjs": "^6.32.2",
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@ncontiero/eslint-config": "6.2.0",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.5.2",
    "@types/prompts": "^2.4.9",
    "@types/react": "^19.1.14",
    "colorette": "^2.0.20",
    "dkcutter": "5.0.2",
    "eslint": "^9.36.0",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.1",
    "next": "15.5.4",
    "next-auth": "^4.24.11",
    "ora": "^9.0.0",
    "prompts": "^2.4.2",
    "react": "^19.1.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.6",
    "type-fest": "^5.0.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "zod": "^4.1.11"
  },
  "lint-staged": {
    "*": "pnpm lint:fix"
  }
}
