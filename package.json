{
  "name": "dkcutter-nextjs",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.26.0",
  "description": "DKCutter NextJs is a framework for quickly starting Next.Js projects.",
  "author": "Nicolas Contiero <https://github.com/ncontiero>",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "generate": "dkcutter .",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "check": "pnpm lint && pnpm typecheck",
    "test": "vitest",
    "test:strict": "GET_WARNINGS=true pnpm test",
    "commitlint": "commitlint --edit",
    "pre-commit": "lint-staged -c ./package.json",
    "prepare": "husky"
  },
  "devDependencies": {
    "@clerk/nextjs": "^6.36.3",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@ncontiero/eslint-config": "6.5.2",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@trigger.dev/sdk": "4.3.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.4",
    "@types/prompts": "^2.4.9",
    "@types/react": "19.2.7",
    "better-auth": "1.4.7",
    "colorette": "^2.0.20",
    "dkcutter": "5.0.6",
    "eslint": "^9.39.2",
    "execa": "^9.6.1",
    "fs-extra": "^11.3.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "next": "16.0.10",
    "next-auth": "5.0.0-beta.30",
    "ora": "^9.0.0",
    "prompts": "^2.4.2",
    "react": "19.2.3",
    "tailwindcss": "4.1.18",
    "tsx": "^4.21.0",
    "type-fest": "^5.3.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15",
    "zod": "^4.2.1"
  },
  "lint-staged": {
    "*": "pnpm lint:fix"
  }
}
