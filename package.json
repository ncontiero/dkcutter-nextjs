{
  "name": "dkcutter-nextjs",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.11.0",
  "description": "DKCutter NextJs is a framework for quickly starting Next.Js projects.",
  "author": "Nicolas Contiero <https://github.com/ncontiero>",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "generate": "dkcutter .",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "check": "pnpm lint && pnpm typecheck",
    "test": "vitest",
    "commitlint": "commitlint --edit",
    "pre-commit": "lint-staged -c ./package.json",
    "prepare": "husky"
  },
  "devDependencies": {
    "@clerk/nextjs": "^6.20.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@ncontiero/eslint-config": "5.2.0",
    "@t3-oss/env-nextjs": "^0.13.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.29",
    "@types/prompts": "^2.4.9",
    "@types/react": "^19.1.6",
    "colorette": "^2.0.20",
    "dkcutter": "5.0.0",
    "eslint": "^9.27.0",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "next": "15.3.3",
    "next-auth": "^4.24.11",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "react": "^19.1.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.4",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4",
    "zod": "^3.25.42"
  },
  "lint-staged": {
    "*": "pnpm lint:fix"
  }
}
